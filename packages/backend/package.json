{
  "name": "@init/backend",
  "private": true,
  "license": "MIT",
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    "./api": "./src/functions/_generated/api.js",
    "./client": "./src/client/index.ts",
    "./client/*": "./src/client/*.ts",
    "./client/auth": "./src/client/auth/index.ts",
    "./client/auth/*": "./src/client/auth/*.ts"
  },
  "scripts": {
    "bump:deps": "bun update --interactive",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "convex dev",
    "env:set": "convex env set",
    "typecheck": "tsc --noEmit",
    "generate:auth": "bun run ./scripts/generate-schema.ts"
  },
  "dependencies": {
    "@convex-dev/better-auth": "0.10.10",
    "@convex-dev/react-query": "0.1.0",
    "@init/auth": "workspace:*",
    "@init/env": "workspace:*",
    "@init/error": "workspace:*",
    "@init/observability": "workspace:*",
    "@init/utils": "workspace:*",
    "@tanstack/react-query": "^5.90.20",
    "convex": "1.31.6",
    "convex-helpers": "0.1.111"
  },
  "devDependencies": {
    "@tooling/tsconfig": "workspace:*",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": "*"
  }
}
