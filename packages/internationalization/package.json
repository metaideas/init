{
  "name": "@init/internationalization",
  "private": true,
  "license": "MIT",
  "type": "module",
  "exports": {
    "./translations/*": "./translations/*.json",
    "./plugin": "./src/plugin.ts",
    "./*": "./src/_generated/*.js"
  },
  "scripts": {
    "bump:deps": "bun update --interactive",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "translations:check": "i18n-check --source en --locales translations",
    "translations:generate": "inlang machine translate --project project.inlang",
    "typecheck": "adamantite typecheck",
    "typegen": "bunx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/_generated"
  },
  "devDependencies": {
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.5.0",
    "@lingual/i18n-check": "0.8.14",
    "@tooling/tsconfig": "workspace:*",
    "typescript": "catalog:"
  }
}
