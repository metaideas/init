  },
  "devDependencies": {
    "@tooling/tsconfig": "workspace:*",
    "@types/react": "catalog:",
    "react": "catalog:",
    "react-email": "3.0.6",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "catalog:"
  }
