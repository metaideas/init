{
  "name": "init",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "tooling/*"
  ],
  "scripts": {
    "boundaries": "turbo boundaries",
    "build": "turbo build",
    "clean": "turbo clean && git clean -xdf .cache .turbo dist node_modules",
    "db:migrate": "turbo --filter @init/db migrate",
    "db:push": "turbo --filter @init/db push",
    "db:studio": "turbo --filter @init/db studio",
    "deps:check": "taze -rl",
    "deps:list": "syncpack list",
    "deps:mismatch": "syncpack list-mismatches --types dev,prod",
    "deps:sync": "syncpack fix-mismatches --types dev,prod",
    "deps:sync:prompt": "syncpack prompt --types dev,prod",
    "deps:update": "taze -rlw",
    "dev": "turbo dev",
    "docker:down": "docker compose -f infra/local/docker-compose.yml down",
    "docker:up": "docker compose -f infra/local/docker-compose.yml up -d",
    "format": "adamantite format",
    "generate": "turbo generate",
    "lint": "adamantite lint",
    "lint:monorepo": "adamantite monorepo",
    "start": "turbo run start",
    "template": "bun run scripts/index.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "turbo typecheck --continue"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@octokit/rest": "^22.0.0",
    "@turbo/gen": "2.5.4",
    "@types/bun": "1.2.17",
    "adamantite": "0.3.4",
    "commander": "14.0.0",
    "dependency-cruiser": "16.10.3",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "shadcn": "2.7.0",
    "sherif": "1.6.1",
    "syncpack": "13.0.4",
    "taze": "19.1.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3"
  },
  "packageManager": "bun@1.2.17",
  "engines": {
    "node": ">=24"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "@upstash/qstash-cli",
    "docs",
    "extension"
  ]
}
