{
  "name": "init",
  "version": "0.11.1",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "tooling/*"
    ],
    "catalog": {
      "@types/bun": "1.3.3",
      "@types/react": "19.1.13",
      "@types/react-dom": "19.1.9",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "typescript": "5.9.3",
      "vite": "7.2.6"
    }
  },
  "scripts": {
    "boundaries": "turbo boundaries",
    "build": "turbo build",
    "bump:deps": "bun update --interactive --recursive",
    "check": "adamantite check",
    "clean": "turbo clean && git clean -xdf .cache .turbo dist node_modules",
    "db:migrate": "turbo --filter @init/db migrate",
    "db:push": "turbo --filter @init/db push",
    "dev": "turbo run dev",
    "dev:apps": "turbo run dev --filter=!@init/*",
    "dev:packages": "turbo run dev --filter=@init/*",
    "docker:down": "docker compose -f infra/local/docker-compose.yml down",
    "docker:up": "docker compose -f infra/local/docker-compose.yml up -d",
    "fix": "adamantite fix",
    "fix:monorepo": "adamantite monorepo",
    "generate": "bun --bun turbo generate",
    "scripts": "bun run scripts/index.ts",
    "start": "turbo run start",
    "template": "bun --bun run scripts/index.ts template",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "turbo run typecheck --continue",
    "typegen": "turbo run typegen --continue"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@octokit/rest": "22.0.0",
    "@turbo/gen": "2.6.1",
    "@types/bun": "1.3.2",
    "@types/yargs": "17.0.35",
    "adamantite": "0.13.0",
    "consola": "3.4.2",
    "sherif": "1.7.0",
    "turbo": "2.6.3",
    "typescript": "5.9.3",
    "yargs": "18.0.0"
  },
  "packageManager": "bun@1.3.3",
  "engines": {
    "node": ">=24"
  },
  "overrides": {
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "vite": "catalog:"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "@upstash/qstash-cli",
    "docs",
    "extension"
  ]
}
