{
  "name": "init",
  "version": "0.4.0",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "tooling/*"
    ],
    "catalogs": {
      "mobile": {
        "@types/react-dom": "19.1.6",
        "@types/react": "19.0.10",
        "react-dom": "19.0.0",
        "react": "19.0.0",
        "tailwindcss": "3.4.17",
        "typescript": "~5.8.3"
      }
    }
  },
  "scripts": {
    "boundaries": "turbo boundaries",
    "build": "turbo build",
    "clean": "turbo clean && git clean -xdf .cache .turbo dist node_modules",
    "db:migrate": "turbo --filter @init/db migrate",
    "db:push": "turbo --filter @init/db push",
    "db:studio": "turbo --filter @init/db studio",
    "deps:check": "taze -rl",
    "deps:update": "taze -rlw",
    "dev": "turbo dev",
    "docker:down": "docker compose -f infra/local/docker-compose.yml down",
    "docker:up": "docker compose -f infra/local/docker-compose.yml up -d",
    "format": "adamantite format",
    "generate": "turbo generate",
    "lint": "adamantite lint",
    "lint:monorepo": "adamantite monorepo",
    "scripts": "bun run scripts/index.ts",
    "start": "turbo run start",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "turbo typecheck --continue"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@commander-js/extra-typings": "14.0.0",
    "@octokit/rest": "^22.0.0",
    "@turbo/gen": "2.5.6",
    "@types/bun": "1.2.20",
    "adamantite": "^0.7.2",
    "commander": "14.0.0",
    "pino": "9.9.0",
    "pino-pretty": "13.1.1",
    "shadcn": "2.10.0",
    "sherif": "1.6.1",
    "taze": "19.1.0",
    "turbo": "2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "bun@1.2.19",
  "engines": {
    "node": ">=24"
  },
  "overrides": {
    "import-in-the-middle": "1.14.2"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@sentry/cli",
    "@tailwindcss/oxide",
    "@upstash/qstash-cli",
    "docs",
    "extension"
  ]
}
