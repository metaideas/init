# ===========PROJECT===========
# Add specific project environment variables here

BASE_URL="http://localhost:3000"
NODE_ENV="development"

# ===========PACKAGES===========
# Add environment variables for all the packages you are using in the project

# Database
DATABASE_URL="http://127.0.0.1:8080"
DATABASE_AUTH_TOKEN="your-database-auth-token"

# Email
EMAIL_FROM="test@example.com"
RESEND_API_KEY="your-resend-api-key"
MOCK_RESEND="true" # Set to true to use mock resend

# Observability
AXIOM_DATASET="init-development"
AXIOM_TOKEN="your-axiom-token"

SENTRY_DSN="https://your-sentry-dsn.sentry.io"
SENTRY_ORG="your-org"
SENTRY_PROJECT="your-project"
SENTRY_AUTH_TOKEN="your-sentry-auth-token"

# Queue
QSTASH_URL="http://127.0.0.1:8288"
QSTASH_TOKEN="your-qstash-token"
QSTASH_CURRENT_SIGNING_KEY="your-qstash-current-signing-key"
QSTASH_NEXT_SIGNING_KEY="your-qstash-next-signing-key"

# Auth
AUTH_SECRET="your-auth-secret"
BETTER_AUTH_TRUSTED_ORIGINS="http://localhost:3000,http://localhost:3001"

# Auth Providers
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"

# KV
UPSTASH_REDIS_REST_URL="http://localhost:8079"
UPSTASH_REDIS_REST_TOKEN="your-redis-rest-token"
